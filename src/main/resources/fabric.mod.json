{
    "schemaVersion": 1,
    "id": "guard-checker",
    "version": "${version}",
    "name": "Guard Checker",
    "description": "An addon template for Meteor Client addons.",
    "authors": [
        "sushi-obfuscation"
    ],
    "environment": "client",
    "entrypoints": {
        "meteor": [
            "com.github.shiftjis.bungeeguard.GuardCheckerAddon"
        ]
    },
    "mixins": [
        "guard-checker.mixins.json"
    ],
    "custom": {
        "meteor-client:color": "225,25,25"
    },
    "depends": {
        "java": ">=21",
        "minecraft": [
            "${mc_version}"
        ],
        "meteor-client": "*"
    }
}
